`};function iL(e,t,i,a,r){var n,s,o;let l={},d={};switch(e.code){case RT.MEDIA_ERR_NETWORK:switch(l.title=tS("Network Error",r),l.message=e.message,null==(n=e.data)?void 0:n.response.code){case 412:l.title=tS("Video is not currently available",r),l.message=tS("The live stream or video file are not yet ready.",r),d.message=tS("This playback-id may belong to a live stream that is not currently active or an asset that is not ready.",r),d.file="412-not-playable.md";break;case 404:l.title=tS("Video does not exist",r),l.message="",d.message=tS("This playback-id does not exist. You may have used an Asset ID or an ID from a different resource.",r),d.file="404-not-found.md";break;case 403:{if(l.title=tS("Invalid playback URL",r),l.message=tS("The video URL or playback-token are formatted with incorrect or incomplete information.",r),d.message=tS("403 error trying to access this playback URL. If this is a signed URL, you might need to provide a playback-token.",r),d.file="missing-signed-tokens.md",!a)break;let{exp:e,aud:t,sub:n}=oS(a),s=n!==i,o="v"!==t,u={timeStyle:"medium",dateStyle:"medium"};if(Date.now()>1e3*e){l.title=tS("Video URL has expired",r),l.message=tS("The videos secured playback-token has expired.",r),d.message=tS("The videos secured playback-token has expired.",r),d.context=tS("Expired at: {expiredDate}. Current time: {currentDate}.",r).format({expiredDate:new Intl.DateTimeFormat(Jk.code,u).format(1e3*e),currentDate:new Intl.DateTimeFormat(Jk.code,u).format(Date.now())}),d.file="403-expired-token.md";break}if(s){l.title=tS("Video URL is formatted incorrectly",r),l.message=tS("The videos playback ID does not match the one encoded in the playback-token.",r),d.message=tS("The videos playback ID does not match the one encoded in the playback-token.",r),d.context=tS("Specified playback ID: {playbackId} and the playback ID encoded in the playback-token: {tokenPlaybackId}",r).format({playbackId:i,tokenPlaybackId:n}),d.file="403-playback-id-mismatch.md";break}if(o){l.t